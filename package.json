{
  "name": "accelara",
  "version": "1.0.0",
  "type": "commonjs",
  "description": "ACCELARA - Unified HTTP + BitTorrent CLI Download Manager with Electron GUI",
  "main": "electron/main.js",
  "scripts": {
    "dev": "make build-api && concurrently \"npm run dev:react\" \"npm run dev:electron\"",
    "dev:react": "vite",
    "dev:electron": "wait-on http://localhost:5173 && electron .",
    "build": "make build-api && vite build && electron-builder",
    "build:react": "vite build",
    "build:go": "make build",
    "preview": "vite preview",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "keywords": [
    "download-manager",
    "bittorrent",
    "http",
    "electron"
  ],
  "author": "Mwangii Kinuthia",
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.10.2",
    "@electron-forge/maker-deb": "^7.10.2",
    "@electron-forge/maker-dmg": "^7.10.2",
    "@electron-forge/maker-rpm": "^7.10.2",
    "@electron-forge/maker-squirrel": "^7.10.2",
    "@electron-forge/maker-zip": "^7.10.2",
    "@electron-forge/plugin-auto-unpack-natives": "^7.10.2",
    "@electron-forge/plugin-fuses": "^7.10.2",
    "@electron/fuses": "^1.8.0",
    "@electron/rebuild": "^4.0.1",
    "@types/node": "^20.19.24",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.2.1",
    "electron": "^39.1.1",
    "electron-rebuild": "^3.2.9",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.7.2",
    "vite": "^7.2.2",
    "wait-on": "^9.0.3"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "electron-squirrel-startup": "^1.0.1",
    "lucide-react": "^0.462.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^2.15.4",
    "sql.js": "^1.13.0"
  },
  "overrides": {
    "inflight": "npm:inflight@^1.0.6",
    "glob": "^10.0.0",
    "rimraf": "^5.0.0",
    "boolean": "^4.0.0",
    "@npmcli/move-file": "npm:@npmcli/fs@^3.0.0"
  }
}
