{
  "name": "accelara",
  "version": "3.0.3",
  "type": "commonjs",
  "description": "ACCELARA - Unified HTTP + BitTorrent CLI Download Manager with Tauri GUI",
  "productName": "ACCELARA",
  "scripts": {
    "dev": "make build-api && npm run dev:react & npx tauri dev",
    "dev:react": "vite",
    "dev:tauri": "export PATH=\"$HOME/.cargo/bin:$PATH\" && npx tauri dev",
    "build": "make build-api && npm run build:react && npm run verify:resources && bash -c 'export PATH=\"$HOME/.cargo/bin:$PATH\" && npx tauri build'",
    "build:tauri": "make build-api && npm run build:react && npm run verify:resources && bash -c 'export PATH=\"$HOME/.cargo/bin:$PATH\" && npx tauri build'",
    "build:react": "vite build",
    "verify:resources": "node scripts/verify-resources.js",
    "build:go": "make build",
    "preview": "vite preview"
  },
  "keywords": [
    "download-manager",
    "bittorrent",
    "http",
    "tauri"
  ],
  "author": "Mwangii Kinuthia",
  "license": "MIT",
  "devDependencies": {
    "@tauri-apps/api": "^2.9.0",
    "@tauri-apps/cli": "^2.9.4",
    "@types/node": "^20.19.24",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.7.2",
    "vite": "^7.2.2"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "lucide-react": "^0.462.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^2.15.4"
  },
  "overrides": {
    "inflight": "npm:inflight@^1.0.6",
    "glob": "^10.0.0",
    "rimraf": "^5.0.0",
    "boolean": "^4.0.0",
    "@npmcli/move-file": "npm:@npmcli/fs@^3.0.0"
  }
}
